<%= simple_form_for [:admin, current_user_to_edit] do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :type, as: :select, collection: ['Quest', 'Teacher', 'Student', 'Parent'], selected: f.object.type %>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>


