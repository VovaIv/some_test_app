<%= turbo_frame_tag meeting_type do %>
  <div class="row row-cols-2">
    <div class="col">
      <%= link_to meeting_type.name %>
    </div>
    <div class="col">
      <div class="btn-group">
        <%= button_to "Delete",
                        admin_meeting_type_path(meeting_type),
                        method: :delete,
                        class: "btn btn-danger" %>
        <%= link_to "Edit",
                  edit_admin_meeting_type_path(meeting_type),
                  class: "btn btn-secondary" %>

    
      </div>
    </div>
  </div>
<% end %>