<h2 class="accordion-header" id="<%= meeting.id %>">
  <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#<%= dom_id(meeting) %>" aria-expanded="false" aria-controls="<%= dom_id(meeting) %>">
    <%= meeting.name %>
  </button>
</h2>
<div id="<%= dom_id(meeting) %>" class="accordion-collapse collapse" aria-labelledby="<%= meeting.id %>" data-bs-parent="#meetings">
  <div class="accordion-body">
    <div class="card">
      <div class="card-header">
        <%= meeting.notes %> 
        </p>
        <p>Duration of the meeting <%= meeting.duration %></p>
      </div>
      <div class="card-body">      
           <ul>
            <% meeting.meeting_hours.each do |hour| %> 
              <li >Day <%= hour.day_when %> , start:  <%= hour.from_hour.strftime("%I:%M%p") %>, end:  <%= hour.to_hour.strftime("%I:%M%p") %></li>
            <% end %>
          </ul>
      </div>
    </div>

  </div>
</div>
